//Write a program to display the following output using a single cout statement. Maths=90, Physics=74, Chemistry=76

#include <iostream>

int main() {
    int Maths = 90, Physics = 74, Chemistry = 76;
    
    std::cout << "Maths=" << Maths << ", Physics=" << Physics << ", Chemistry=" << Chemistry << std::endl;

    return 0;
}
